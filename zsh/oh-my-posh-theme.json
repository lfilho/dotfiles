  "secondary_prompt": {
    "template": "<p:yellow,transparent>\ue0b6</><,p:yellow>→ </><p:yellow,transparent>\ue0b0</> ",
    "foreground": "p:base",
    "background": "transparent"
  },
  "tooltips": [
    {
      "properties": {
        "display_default": true
      },
      "leading_diamond": "\ue0b0",
      "trailing_diamond": "\ue0b4",
      "template": " \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
      "foreground": "p:text",
      "background": "p:peach",
      "type": "aws",
      "style": "diamond",
      "tips": ["aws"]
    },
    {
      "properties": {
        "display_default": true
      },
      "leading_diamond": "\ue0b0",
      "trailing_diamond": "\ue0b4",
      "template": " \uebd8 {{ .Name }} ",
      "foreground": "p:text",
      "background": "p:sapphire",
      "type": "az",
      "style": "diamond",
      "tips": ["az"]
    }
  ],
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": " \uea83 {{ path .Path .Location }} ",
          "foreground": "p:base",
          "background": "p:sapphire",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_max_length": 25,
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "branch_icon": "\ue725"
          },
          "template": " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "foreground": "p:base",
          "powerline_symbol": "\ue0b0",
          "background": "p:green",
          "type": "git",
          "style": "powerline",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:base{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:base{{ end }}",
            "{{ if gt .Ahead 0 }}p:base{{ end }}"
          ],
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:flamingo{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
            "{{ if gt .Ahead 0 }}p:lavender{{ end }}",
            "{{ if gt .Behind 0 }}p:maroon{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf0e7 ",
          "foreground": "p:red",
          "powerline_symbol": "\ue0b0",
          "background": "p:surface1",
          "type": "root",
          "style": "powerline"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "leading_diamond": "<transparent,background>\ue0b0</>",
      "trailing_diamond": "\ue0b4",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "round",
            "threshold": 1500
          },
          "trailing_diamond": "\ue0b0",
          "foreground": "p:sapphire",
          "background": "p:surface1",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": " {{ if gt .Code 0 }}\uf00d{{ else }}\uf00c{{ end }} ",
          "foreground": "p:base",
          "background": "p:teal",
          "type": "status",
          "style": "diamond",
          "background_templates": ["{{ if gt .Code 0 }}p:red{{ end }}"]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "filler": "—",
      "foreground_templates": ["{{ if gt .Code 0 }}p:red{{ end }}"],
      "background_template": ["{{ if gt .Code 0 }}p:red{{ end }}"],
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "style": "austin"
          },
          "foreground": "p:crust",
          "powerline_symbol": "\ue0b6",
          "trailing_diamond": "\ue0b1",
          "background": "p:teal",
          "type": "executiontime",
          "style": "powerline",
          "background_templates": ["{{ if gt .Code 0 }}p:red{{ end }}"],
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "template": " {{ .CurrentDate | date .Format }} ",
          "foreground": "p:crust",
          "powerline_symbol": "\ue0b6",
          "background": "p:mauve",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "❯",
          "foreground": "p:green",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}p:red{{ end }}"]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
